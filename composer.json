{
    "name": "fnsc/laravel-google-drive",
    "description": "Filesystem adapter for Google Drive",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fnsc\\LaravelGoogleDrive\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gabriel Fonseca",
            "email": "gabrieldfnsc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "leroy-merlin-br/coding-standard": "dev-chore/bump-packages",
        "rector/rector": "^0.12.21",
        "phpstan/phpstan": "^1.5",
        "phpro/grumphp": "^1.11"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
