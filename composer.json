{
    "name": "fnsc/laravel-google-drive",
    "description": "Filesystem adapter for Google Drive",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Fonseca",
            "email": "gabrieldfnsc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "ext-simplexml": "*",
        "illuminate/support": "^9.34",
        "illuminate/console": "^9.34",
        "illuminate/config": "^9.34",
        "google/apiclient": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "leroy-merlin-br/coding-standard": "v3.1.0",
        "rector/rector": "^0.12.23",
        "phpstan/phpstan": "^1.8",
        "phpro/grumphp": "^1.13",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.9"
    },
    "autoload": {
        "psr-4": {
            "LaravelGoogleDrive\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "LaravelGoogleDrive\\": "tests/Unit/src"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
